{
  "name": "laravel-model-collection",
  "version": "0.0.1",
  "description": "A JS library to fetch, save/update and delete records from a Laravel API.",
  "main": "main.js",
  "dependencies": {
    "axios": "^0.19.0",
    "lodash": "^4.17.11",
    "pluralize": "^8.0.0"
  },
  "devDependencies": {
    "browserify": "^16.3.0"
  },
  "scripts": {
    "development": "tsc",
    "dev": "npm run development",
    "watch": "tsc -w",
    "production": "tsc && browserify ./dist/src/main.js -o ./dist/browserify/lmc.js",
    "prod": "npm run production",
    "test": "node ./dist/src/tests.js",
    "build-test": "npm run development && npm run test"
  },
  "keywords": [
    "laravel",
    "js",
    "vue",
    "vue.js",
    "vuejs",
    "api"
  ],
  "author": "Gabriel Prieto Cassano",
  "license": "MIT"
}